file = {:C$\backslash$:/Users/Min-Yang.Lee/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Ropicki, Larkin - 2014 - Social Network Analysis of Price Dispersion in Fishing Quota Lease Markets.pdf:pdf},
