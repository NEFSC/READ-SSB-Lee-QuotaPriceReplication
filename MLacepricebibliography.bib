doi = {10.1080/08920753.2018.1522495},
file = {:C$\backslash$:/Users/Min-Yang.Lee/Downloads/Crew in the West Coast Groundfish Catch Share Program Changes in Compensation and Job Satisfaction.pdf:pdf},
