journal = {Fisheries Research},
keywords = {Catch shares,Distributional effects,Economic indicators,Fisheries,Performance measures},
pages = {213--223},
title = {{Performance of federally managed catch share fisheries in the United States}},
volume = {179},
year = {2016}
