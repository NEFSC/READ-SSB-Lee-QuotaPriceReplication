journal = {Food Policy},
keywords = {Control function,Ethiopia,Improved chickpea,Poverty,Technology adoption},
pages = {50--61},
pmid = {28148997},
publisher = {International Crops Research Institute for the Semi-Arid Tropics (ICRISAT)},
title = {{Welfare impacts of improved chickpea adoption: A pathway for rural development in Ethiopia?}},
url = {http://dx.doi.org/10.1016/j.foodpol.2016.11.007},
volume = {66},
year = {2017}
